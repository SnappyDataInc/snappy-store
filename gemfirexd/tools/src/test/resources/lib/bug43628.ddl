
CREATE TABLE CLIENTE ( 
            ID_CLIENTE                 BIGINT NOT NULL,
            NOMBRE                     VARCHAR(20) NOT NULL,
            ID_PROTOCOLO                       DECIMAL(2,0) NOT NULL,
            MONEDA                     DECIMAL(2,0) NOT NULL,
            TIMEOUT_CLIENTE                   DECIMAL(10,0) NOT NULL,
            TIMEOUT_PPAS                       DECIMAL(10,0) NOT NULL,
            TTL_REVERSO                         DECIMAL(10,0) DEFAULT 0 NOT NULL,
            CVE_GSM_ABO                       VARCHAR(10) NOT NULL,
            CVE_GSM_REV                        VARCHAR(10) NOT NULL,
            CVE_GSM_COMISION              VARCHAR(10) NOT NULL,
            CVE_GSM_COMISION_REV                  VARCHAR(10) NOT NULL,
            UP_DATE_BY                           VARCHAR(10) NOT NULL,
            TRANS_NUMBER_ABO             VARCHAR(10) NOT NULL,
            TRANS_NUMBER_REV             VARCHAR(10) NOT NULL,
            ADJ_SHORT_DESC_COMISION             VARCHAR(15) NOT NULL,
            ADJ_COMMENT_COMISION                  VARCHAR(60) NOT NULL,
            ADJ_SHORT_DESC_COMISION_REV    VARCHAR(15) NOT NULL,
            ADJ_COMMENT_COMISION_REV          VARCHAR(60) NOT NULL,
            COMISION                   DECIMAL(10,2) DEFAULT 0 NOT NULL,
            ACCIONES_VALIDAS                VARCHAR(100) NOT NULL,
            MONTOS_VALIDOS                  VARCHAR(100) NOT NULL,
            REGIONES_VALIDAS                VARCHAR(100) NOT NULL,
            NOTIFICA_ACCION                    DECIMAL(1,0) DEFAULT 0 NOT NULL,
            TIEMPO_PAUSA                       DECIMAL(10,0) DEFAULT 0 NOT NULL,
            MAINT_ACCION                        VARCHAR(100) NOT NULL,
            MAINT_UNDO                           VARCHAR(100) NOT NULL,
            TIPO_BOLSA                            DECIMAL(2,0) DEFAULT 0 NOT NULL,
            FLAGS                         DECIMAL(16,0) NOT NULL,
            RFC                              VARCHAR(15) NULL,
            ORIGIN_NODE_TYPE                VARCHAR(20) NULL 
            ) 
PARTITION BY COLUMN (ID_CLIENTE);

CREATE TABLE ACCION ( 
            ID_ACCION                   DECIMAL(10,0) NOT NULL,
            TIPO_ACCION               DECIMAL(3,0) NOT NULL,
            INTERNA                      DECIMAL(1,0) NOT NULL,
            PRINCIPAL                   DECIMAL(10,2) NULL,
            SUB_CTA1                    DECIMAL(10,2) NULL,
            SUB_CTA2                    DECIMAL(10,2) NULL,
            SUB_CTA3                    DECIMAL(10,2) NULL,
            SUB_CTA4                    DECIMAL(10,2) NULL,
            SUB_CTA5                    DECIMAL(10,2) NULL,
            SUB_CTA6                    DECIMAL(10,2) NULL,
            SUB_CTA7                    DECIMAL(10,2) NULL,
            SUB_CTA8                    DECIMAL(10,2) NULL,
            SUB_CTA9                    DECIMAL(10,2) NULL,
            SUB_CTA10                  DECIMAL(10,2) NULL,
            SUPERVISION_DATE                DECIMAL(10,0) NULL,
            SERVICE_FEE_DATE               DECIMAL(10,0) NULL,
            SERVICE_REMOVAL_DATE      DECIMAL(10,0) NULL,
            CREDIT_CLEARANCE_DATE     DECIMAL(10,0) NULL,
            SUB_CTA1_DATE        DECIMAL(10,0) NULL,
            SUB_CTA2_DATE        DECIMAL(10,0) NULL,
            SUB_CTA3_DATE        DECIMAL(10,0) NULL,
            SUB_CTA4_DATE        DECIMAL(10,0) NULL,
            SUB_CTA5_DATE        DECIMAL(10,0) NULL,
            SUB_CTA6_DATE        DECIMAL(10,0) NULL,
            SUB_CTA7_DATE        DECIMAL(10,0) NULL,
            SUB_CTA8_DATE        DECIMAL(10,0) NULL,
            SUB_CTA9_DATE        DECIMAL(10,0) NULL,
            SUB_CTA10_DATE       DECIMAL(10,0) NULL,
            TIPO_CAMBIO              DECIMAL(10,2) NULL,
            COMISION                    DECIMAL(10,2) NULL,
            BOLSA                         DECIMAL(17,2) NULL,
            FECHA                         TIMESTAMP DEFAULT CURRENT_TIMESTAMP NOT NULL,
            MES                             DECIMAL(2,0) NULL,
			ID_CLIENTE              BIGINT NOT NULL
            ) 
PARTITION BY COLUMN (ID_CLIENTE)
COLOCATE WITH (CLIENTE);

CREATE TABLE ADMINISTRACION ( 
            PARAMETRO    VARCHAR(20) NOT NULL,
            DESCRIPCION  VARCHAR(100) NOT NULL,
            VALOR             VARCHAR(100) NOT NULL,
            ARGS               VARCHAR(100) NULL,
            ESTATUS         DECIMAL(2,0) DEFAULT 0 NOT NULL 
            )
REPLICATE;

CREATE TABLE AUTENTICA ( 
            PUERTO_SERVICIO      DECIMAL(5,0) NOT NULL,
            IP_CLIENTE                  VARCHAR(23) NOT NULL,
            CODIGO_CLIENTE        VARCHAR(10) NOT NULL,
            ID_CLIENTE                  BIGINT NOT NULL,
            ESTATUS_CONEXION   DECIMAL(2,0) DEFAULT 0 NOT NULL 
            )
REPLICATE;

CREATE TABLE BALANCE ( 
            ID_BALANCE                DECIMAL(10,0) NOT NULL,
            PRINCIPAL                   DECIMAL(10,2) NOT NULL,
            SUB_CTA1                    DECIMAL(10,2) NULL,
            SUB_CTA2                    DECIMAL(10,2) NULL,
            SUB_CTA3                    DECIMAL(10,2) NULL,
            SUB_CTA4                    DECIMAL(10,2) NULL,
            SUB_CTA5                    DECIMAL(10,2) NULL,
            SUB_CTA6                    DECIMAL(10,2) NULL,
            SUB_CTA7                    DECIMAL(10,2) NULL,
            SUB_CTA8                    DECIMAL(10,2) NULL,
            SUB_CTA9                    DECIMAL(10,2) NULL,
            SUB_CTA10                  DECIMAL(10,2) NULL,
            SUPERVISION_DATE                VARCHAR(14) NOT NULL,
            SERVICE_FEE_DATE               VARCHAR(14) NULL,
            SERVICE_REMOVAL_DATE      VARCHAR(14) NULL,
            CREDIT_CLEARANCE_DATE     VARCHAR(14) NULL,
            SUB_CTA1_DATE        VARCHAR(14) NULL,
            SUB_CTA2_DATE        VARCHAR(14) NULL,
            SUB_CTA3_DATE        VARCHAR(14) NULL,
            SUB_CTA4_DATE        VARCHAR(14) NULL,
            SUB_CTA5_DATE        VARCHAR(14) NULL,
            SUB_CTA6_DATE        VARCHAR(14) NULL,
            SUB_CTA7_DATE        VARCHAR(14) NULL,
            SUB_CTA8_DATE        VARCHAR(14) NULL,
            SUB_CTA9_DATE        VARCHAR(14) NULL,
            SUB_CTA10_DATE       VARCHAR(14) NULL,
            ESTATUS                     DECIMAL(2,0) NOT NULL,
            PLAN                            DECIMAL(8,0) NOT NULL,
            FECHA                         TIMESTAMP DEFAULT CURRENT_TIMESTAMP NOT NULL,
            MES                             DECIMAL(2,0) NULL,
			ID_CLIENTE              BIGINT NOT NULL
            )
PARTITION BY COLUMN (ID_CLIENTE)
COLOCATE WITH (CLIENTE);

CREATE TABLE BUZON ( 
            TELEFONO      DECIMAL(12,0) NOT NULL,
            TIPO_ACCION               DECIMAL(3,0) NOT NULL,
            AUTORIZADOR             DECIMAL(7,0) DEFAULT 0 NULL,
            ID_TRANSACCION        BIGINT DEFAULT 0 NULL,
            ESTATUS         DECIMAL(10,0) DEFAULT 0 NULL 
            )
REPLICATE;

CREATE TABLE DPROMOCION ( 
            ID_PROMOCION           DECIMAL(10,0) NOT NULL,
            ID_GRUPO        DECIMAL(10,0) NOT NULL 
            )
REPLICATE;

CREATE TABLE GRUPO ( 
            ID_GRUPO        DECIMAL(10,0) NOT NULL,
            DESCRIPCION  VARCHAR(100) NOT NULL 
            )
REPLICATE;

CREATE TABLE DRESULTADO ( 
            ID_CLIENTE      BIGINT NOT NULL,
            ID_RESULTADO            DECIMAL(10,0) NOT NULL,
            CODIGO           CHAR(2) NOT NULL 
            )
PARTITION BY COLUMN (ID_CLIENTE)
COLOCATE WITH (CLIENTE);

CREATE TABLE PLAN ( 
            ID_GRUPO        DECIMAL(10,0) NOT NULL,
            PLAN                DECIMAL(8,0) NOT NULL,
            REGION            DECIMAL(2,0) NOT NULL,
            TECNOLOGIA   DECIMAL(2,0) NOT NULL,
            DESCRIPCION  VARCHAR(50) NOT NULL,
            SUB_CTAS       DECIMAL(1,0) NOT NULL 
            )
REPLICATE;

CREATE TABLE PROMOCION ( 
            ID_PROMOCION           DECIMAL(10,0) NOT NULL,
            MONEDA          DECIMAL(2,0) NOT NULL,
            MONTO_INI       DECIMAL(10,2) NOT NULL,
            MONTO_FIN      DECIMAL(10,2) NOT NULL,
            REGION_INI      DECIMAL(2,0) NOT NULL,
            REGION_FIN     DECIMAL(2,0) NOT NULL,
            TECNOLOGIA  DECIMAL(2,0) NOT NULL,
            VG_NORMAL   VARCHAR(5) NOT NULL,
            VG_SUB_CTA  VARCHAR(5) NOT NULL,
            VIGENCIA         DECIMAL(5,0) NOT NULL,
            SUB_CTA1        DECIMAL(10,2) DEFAULT 0 NOT NULL,
            SUB_CTA2        DECIMAL(10,2) DEFAULT 0 NOT NULL,
            SUB_CTA3        DECIMAL(10,2) DEFAULT 0 NOT NULL,
            SUB_CTA4        DECIMAL(10,2) DEFAULT 0 NOT NULL,
            SUB_CTA5        DECIMAL(10,2) DEFAULT 0 NOT NULL,
            SUB_CTA6        DECIMAL(10,2) DEFAULT 0 NOT NULL,
            SUB_CTA7        DECIMAL(10,2) DEFAULT 0 NOT NULL,
            SUB_CTA8        DECIMAL(10,2) DEFAULT 0 NOT NULL,
            SUB_CTA9        DECIMAL(10,2) DEFAULT 0 NOT NULL,
            SUB_CTA10      DECIMAL(10,2) DEFAULT 0 NOT NULL,
            FECHA             TIMESTAMP DEFAULT CURRENT_TIMESTAMP NOT NULL 
            )
REPLICATE;

CREATE TABLE RESULTADO ( 
            ID_RESULTADO            DECIMAL(10,0) NOT NULL,
            DESCRIPCION VARCHAR(100) NOT NULL 
            )
REPLICATE;

CREATE TABLE SERVIDOR ( 
            ID_SERVIDOR   DECIMAL(10,0) NOT NULL,
            TECNOLOGIA   DECIMAL(2,0) NOT NULL,
            REGION            DECIMAL(2,0) NOT NULL,
            ESTATUS         DECIMAL(2,0) NOT NULL,
            IP                     VARCHAR(23) NOT NULL,
            PUERTO           DECIMAL(5,0) NOT NULL,
            INSTANCIA       VARCHAR(50) NULL,
            USUARIO          VARCHAR(65) NULL,
            PASSWORD     VARCHAR(65) NULL 
            )
REPLICATE;

CREATE TABLE TRANSACCION ( 
            ID_TRANSACCION        BIGINT NOT NULL,  --original 38   max 31
            ID_CLIENTE      BIGINT NOT NULL,
            FOLIO               DECIMAL(6,0) NOT NULL,
            FECHA_TELCEL           TIMESTAMP NOT NULL,
            TELEFONO      DECIMAL(12,0) NOT NULL,
            REGION            DECIMAL(2,0) NULL,
            TECNOLOGIA               DECIMAL(2,0) NULL,
            MONTO            DECIMAL(10,2) NOT NULL,
            FECHA             DECIMAL(8,0) NOT NULL,
            HORA               DECIMAL(6,0) NOT NULL,
            FECHA_LOCAL             DECIMAL(8,0) NOT NULL,
            HORA_LOCAL               DECIMAL(6,0) NOT NULL,
            ID_MENSAJE                VARCHAR(30) NOT NULL,
            ID_CCOMERCIAL          VARCHAR(30) NOT NULL,
            ID_TIENDA        VARCHAR(30) NOT NULL,
            ID_CAJA           VARCHAR(30) NOT NULL,
            SECUENCIA                 VARCHAR(12) NOT NULL,
            ID_RESULTADO            DECIMAL(10,0) NOT NULL,
            TTL                   DECIMAL(10,0) NULL,
            ID_ACCION       DECIMAL(10,0) NULL,
            ID_ACCION_REV           DECIMAL(10,0) NULL,
            ID_BALANCE                DECIMAL(10,0) NULL,
            ID_BALANCE_REV        DECIMAL(10,0) NULL,
            FECHA_REVERSO       TIMESTAMP NULL,
            MES                 DECIMAL(2,0) NULL,
            NHORA             DECIMAL(2,0) NULL 
            )
PARTITION BY COLUMN (ID_CLIENTE)
COLOCATE WITH (CLIENTE);

CREATE TABLE HISTORICO_REVERSOS ( 
            ID_TRANSACCION        BIGINT NOT NULL,
            FECHA_TELCEL           TIMESTAMP NOT NULL,
            INTERNO          VARCHAR(1) NOT NULL,
            PUERTO           DECIMAL(8,0) NOT NULL 
            )
PARTITION BY COLUMN (ID_TRANSACCION)
COLOCATE WITH (TRANSACCION);

CREATE TABLE REVERSOS ( 
            ID_TRANSACCION        BIGINT NOT NULL,
            FECHA_TELCEL           TIMESTAMP NOT NULL,
            PUERTO           DECIMAL(8,0) NOT NULL 
            )
PARTITION BY COLUMN (ID_TRANSACCION)
COLOCATE WITH (TRANSACCION);


CREATE TABLE TRANSACCION2 ( 
            ID_TRANSACCION        BIGINT NOT NULL,  --original 38   max 31
            ID_CLIENTE      BIGINT NOT NULL,
            FOLIO               DECIMAL(6,0) NOT NULL,
            FECHA_TELCEL           TIMESTAMP NOT NULL,
            TELEFONO      DECIMAL(12,0) NOT NULL,
            REGION            DECIMAL(2,0) NULL,
            TECNOLOGIA               DECIMAL(2,0) NULL,
            MONTO            DECIMAL(10,2) NOT NULL,
            FECHA             DECIMAL(8,0) NOT NULL,
            HORA               DECIMAL(6,0) NOT NULL,
            FECHA_LOCAL             DECIMAL(8,0) NOT NULL,
            HORA_LOCAL               DECIMAL(6,0) NOT NULL,
            ID_MENSAJE                VARCHAR(30) NOT NULL,
            ID_CCOMERCIAL          VARCHAR(30) NOT NULL,
            ID_TIENDA        VARCHAR(30) NOT NULL,
            ID_CAJA           VARCHAR(30) NOT NULL,
            SECUENCIA                 VARCHAR(12) NOT NULL,
            ID_RESULTADO            DECIMAL(10,0) NOT NULL,
            TTL                   DECIMAL(10,0) NULL,
            ID_ACCION       DECIMAL(10,0) NULL,
            ID_ACCION_REV           DECIMAL(10,0) NULL,
            ID_BALANCE                DECIMAL(10,0) NULL,
            ID_BALANCE_REV        DECIMAL(10,0) NULL,
            FECHA_REVERSO       TIMESTAMP NULL,
            MES                 DECIMAL(2,0) NULL,
            NHORA             DECIMAL(2,0) NULL 
            )
PARTITION BY COLUMN (ID_CLIENTE);

CREATE TABLE REVERSOS2 ( 
            ID_TRANSACCION        BIGINT NOT NULL primary key,
            FECHA_TELCEL           TIMESTAMP NOT NULL,
            PUERTO           DECIMAL(8,0) NOT NULL 
            )
PARTITION BY COLUMN (ID_TRANSACCION)
COLOCATE WITH (TRANSACCION2);



