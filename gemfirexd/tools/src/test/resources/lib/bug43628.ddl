ALTER TABLE GRUPO
        ADD CONSTRAINT PK_GRUPO
        PRIMARY KEY (ID_GRUPO)
        ;
ALTER TABLE ADMINISTRACION
        ADD CONSTRAINT PK_ADMINISTRACION
        PRIMARY KEY (PARAMETRO)
        ;
ALTER TABLE AUTENTICA
        ADD CONSTRAINT PK_AUTENTICA
        PRIMARY KEY (PUERTO_SERVICIO, IP_CLIENTE, CODIGO_CLIENTE)
        ;
ALTER TABLE BUZON
        ADD CONSTRAINT PK_BUZON
        PRIMARY KEY (TELEFONO)
        ;
ALTER TABLE CLIENTE
        ADD CONSTRAINT PK_CLIENTE
        PRIMARY KEY (ID_CLIENTE)
        ;
ALTER TABLE DPROMOCION
        ADD CONSTRAINT PK_DPROMOCION
        PRIMARY KEY (ID_PROMOCION, ID_GRUPO)
        ;
ALTER TABLE DRESULTADO
        ADD CONSTRAINT PK_DRESULTADO
        PRIMARY KEY (ID_CLIENTE, ID_RESULTADO)
        ;
ALTER TABLE HISTORICO_REVERSOS
        ADD CONSTRAINT SYS_C0026539
        PRIMARY KEY (ID_TRANSACCION)
        ;
ALTER TABLE PLAN
        ADD CONSTRAINT PK_PLAN
        PRIMARY KEY (PLAN, REGION, TECNOLOGIA)
        ;
ALTER TABLE PROMOCION
        ADD CONSTRAINT PK_PROMOCION
        PRIMARY KEY (ID_PROMOCION)
        ;
ALTER TABLE RESULTADO
        ADD CONSTRAINT PK_RESULTADO
        PRIMARY KEY (ID_RESULTADO)
        ;
ALTER TABLE REVERSOS
        ADD CONSTRAINT SYS_C0026543
        PRIMARY KEY (ID_TRANSACCION)
        ;
ALTER TABLE SERVIDOR
        ADD CONSTRAINT PK_SERVIDOR
        PRIMARY KEY (ID_SERVIDOR)
        ;

ALTER TABLE ACCION
        ADD CONSTRAINT PK_ACCION
        PRIMARY KEY (ID_ACCION);

ALTER TABLE BALANCE
        ADD CONSTRAINT PK_BALANCE
        PRIMARY KEY (ID_BALANCE);


